{
    "name": "dpi/drupal-entity-traits",
    "description": "Useful traits for Drupal content entities.",
    "homepage": "https://github.com/dpi/drupal-entity-traits",
    "license": "BSD-3-Clause",
    "type": "package",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "dpi/drupal-phpunit-bootstrap": "^9.4",
        "drupal/core": ">=9.3",
        "drupal/date_recur": "^3.2@beta",
        "friendsofphp/php-cs-fixer": "dev-master#075082e",
        "jangregor/phpstan-prophecy": "^1",
        "mikey179/vfsstream": "^1.6.7",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1",
        "phpstan/phpstan-phpunit": "1.0.x-dev",
        "phpstan/phpstan-symfony": "^1",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "dpi\\DrupalEntityTraits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dpi\\DrupalEntityTraits\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "scripts": {
        "lintandtest": [
            "./vendor/bin/php-cs-fixer --allow-risky=yes fix",
            "./vendor/bin/phpstan analyse",
            "./vendor/bin/phpunit"
        ]
    }
}
